        </com.simdo.g73cs.View.MyNestedScrollView>

        <LinearLayout
            android:id="@+id/ll_auto_trace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            android:background="@drawable/corner15_ffffff"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="10dp"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:orientation="vertical"
                >

                <TextView
                    android:id="@+id/tv_prompt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="5dp"
                    android:text="请输入自动定位的imsi"
                    android:textColor="@color/color_1a1a1a"
                    android:textSize="20sp" />

                <AutoCompleteTextView
                    android:id="@+id/ed_auto_imsi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/bg_edittext"
                    android:digits="0123456789"
                    android:inputType="number"
                    android:maxLength="15"
                    android:textColor="@color/color_1a1a1a"
                    android:textCursorDrawable="@drawable/edit_cursor_color" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginStart="5dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="@mipmap/iv_auto_bottom_hint" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:gravity="center"
                        android:text=" imsi为15位数字，否则无法自动定位！" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/btn_ok"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1"
                    android:background="@drawable/btn_bg_ok"
                    android:gravity="center"
                    android:paddingVertical="10dp"
                    android:text="@string/confirm"
                    android:textColor="@color/white"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/btn_cancel"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1"
                    android:background="@drawable/btn_bg_cancel_2"
                    android:gravity="center"
                    android:paddingVertical="10dp"
                    android:text="@string/cancel"
                    android:textColor="@color/color_1c1c1c"
                    android:textSize="18sp" />
            </LinearLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Switch
            android:id="@+id/sw_auto_trace"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="15dp"
            android:switchMinWidth="99dp"
            android:textColor="@color/button_text_color_confirm_2"
            android:thumb="@drawable/switch_thumb3"
            android:track="@drawable/switch_track2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tv_do_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="UseSwitchCompatOrMaterialXml" />

        <TextView
            android:id="@+id/tv_switch_text_cfg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="2dp"
            android:text="手动"
            android:textColor="@color/button_bg_color_confirm"
            android:textSize="18dp"
            app:layout_constraintBottom_toBottomOf="@+id/sw_auto_trace"
            app:layout_constraintEnd_toEndOf="@+id/sw_auto_trace"
            app:layout_constraintStart_toStartOf="@id/sw_auto_trace"
            app:layout_constraintTop_toTopOf="@+id/sw_auto_trace" />

        <TextView
            android:id="@+id/tv_switch_text_auto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="2dp"
            android:text="自动"
            android:textColor="@color/switch_close_color2"
            android:textSize="18dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/sw_auto_trace"
            app:layout_constraintEnd_toEndOf="@+id/sw_auto_trace"
            app:layout_constraintStart_toStartOf="@id/sw_auto_trace"
            app:layout_constraintTop_toTopOf="@+id/sw_auto_trace" />

        <TextView
            android:id="@+id/tv_do_btn"
            android:layout_width="0dp"
            android:layout_height="36dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/btn_bg_ok"
            android:gravity="center"
            android:minWidth="200dp"
            android:text="@string/open"
            android:textColor="@color/button_text_color_confirm"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/sw_auto_trace"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>
